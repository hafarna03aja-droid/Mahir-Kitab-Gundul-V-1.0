{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "lib": ["ES2022"],
    "skipLibCheck": true,
    "types": ["node"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": false,
    "noEmit": false,
    "outDir": "./dist",
    "allowImportingTsExtensions": false
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
